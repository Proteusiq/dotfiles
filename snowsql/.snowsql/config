[connections]
# Default connection (override with snowsql -c <name>)
# accountname = <account_identifier>
# username = <username>
# password = <password>
# dbname = <database>
# schemaname = <schema>
# warehousename = <warehouse>
# rolename = <role>

# [connections.example]
# accountname = xy12345.us-east-1
# username = my_user
# dbname = my_database
# schemaname = public
# warehousename = compute_wh

[options]
# Auto-completion
auto_completion = True

# Output format: psql, plain, csv, tsv, json
output_format = psql

# Disable auto-upgrade (managed via Homebrew)
noup = True

# Friendly prompt
prompt_format = [#FFFF00][user]@[account].[database].[schema]>[#00FF00]

# Timing
timing = True

# Row count
rowset_size = 1000

[variables]
# Custom variables accessible via &<name> in queries
# example_var = example_value
